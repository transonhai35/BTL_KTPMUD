  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start --watch",
    "dev": "yarn start:dev",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky",
    "migration:run": "yarn typeorm migration:run -- -d ./src/ormconfig.ts",
    "migration:generate": "yarn typeorm -- -d ./src/ormconfig.ts migration:generate",
    "migration:create": "yarn typeorm -- migration:create",
    "migration:revert": "yarn typeorm -- -d ./src/ormconfig.ts migration:revert",
    "seed:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config -n ./src/ormconfig.ts",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed -n ./src/ormconfig.ts",
    "doc": "npx compodoc --disableSourceCode -p tsconfig.doc.json -n \"Stampted backend documentation\""
  },
